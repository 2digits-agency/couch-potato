{
  "name": "couch-potato",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "types": "tsc",
    "check": "pnpm lint && pnpm format && pnpm types && pnpm tsc",
    "storybook:ios": "pnpm storybook:generate && EXPO_PUBLIC_STORYBOOK_ENABLED=true expo start --ios",
    "storybook:android": "pnpm storybook:generate && EXPO_PUBLIC_STORYBOOK_ENABLED=true expo start --android",
    "storybook:generate": "sb-rn-get-stories",
    "storybook:watch": "sb-rn-watcher",
    "upgrade": "pnpm upgrade --interactive",
    "upgrade:latest": "pnpm upgrade --interactive --latest"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4.5.1",
    "@react-navigation/native": "^6.0.2",
    "expo": "~49.0.13",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-svg": "13.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-controls": "^6.5.6",
    "@storybook/addon-ondevice-actions": "^6.5.6",
    "@storybook/addon-ondevice-controls": "^6.5.6",
    "@storybook/core-common": "^6.5.6",
    "@storybook/react-native": "^6.5.6",
    "@types/react": "~18.2.14",
    "clsx": "^2.0.0",
    "eslint": "^8.51.0",
    "eslint-config-universe": "^12.0.0",
    "nativewind": "^2.0.11",
    "prettier": "^3.0.3",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.14",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "packageManager": "pnpm@8.9.1"
}
